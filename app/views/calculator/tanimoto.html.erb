
<h3>Lower Triangular matrix </h3>
<table class="table table-striped table-bordered table-condensed">
  <b>
    <tr>
      <td></td><td>1</td><td>2</td><td>3</td><td>4</td><td>5</td><td>6</td>
        
    </tr>
<% @similarities.each_with_index do |data,i| %>
    <tr>
      <td>
        <%= i+1 %>
      </td>    
    <% data.each do |d| %>
        <td>
            <%= d %>
        </td>        
    <% end%>
      </tr>
<% end %>
    </b>
</table>